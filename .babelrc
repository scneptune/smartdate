{
  "presets": ["react-native"],
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "extensions": [
          ".ios.js",
          ".android.js",
          ".json",
          ".js",
        ],
        "root": ["./src/**", "./app_assets/**"],
        "alias": {
          "@containers": "./src/containers",
          "@navigation": "./src/navigation",
          "@libs": "./src/libs"
        }
      }
    ]
  ]
}
